<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.dawnstars.hrmanagersys">

    <application
        android:allowBackup="false"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".management.attendanceInfo.AddAttendanceProjectActivity"
            android:parentActivityName=".management.attendanceInfo.AttendanceProjectActivity" />
        <activity
            android:name=".management.attendanceInfo.VacationSettingActivity"
            android:parentActivityName=".management.MainActivity" />
        <activity
            android:name=".management.attendanceInfo.AttendanceSettingActivity"
            android:parentActivityName=".management.MainActivity" />
        <activity
            android:name=".management.attendanceInfo.AttendanceProjectActivity"
            android:parentActivityName=".management.MainActivity" />
        <activity
            android:name=".management.attendanceInfo.AttendanceClassActivity"
            android:parentActivityName=".management.MainActivity" />
        <activity
            android:name=".management.personnelInfo.DismissActivity"
            android:parentActivityName=".management.MainActivity" />
        <activity
            android:name=".management.personnelInfo.RetireActivity"
            android:parentActivityName=".management.MainActivity" />
        <activity
            android:name=".management.personnelInfo.ProbationActivity"
            android:parentActivityName=".management.MainActivity" />
        <activity
            android:name=".management.personnelInfo.TrialActivity"
            android:parentActivityName=".management.MainActivity" />
        <activity
            android:name=".management.MainActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />

                <action android:name="android.intent.action.VIEW" />
            </intent-filter>
        </activity>
        <activity
            android:name=".ui.login.LoginActivity"
            android:label="@string/title_login"
            android:parentActivityName=".management.MainActivity" />
    </application>

</manifest>